{
  "name": "bamboo",
  "version": "5.0.0",
  "private": true,
  "description": "Discord bot embedded with NetEase Music Api (从网易云播放的discord机器人)",
  "homepage": "https://github.com/k27dong/bamboo#readme",
  "bugs": {
    "url": "https://github.com/k27dong/bamboo/issues",
    "email": "me@kefan.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k27dong/bamboo.git"
  },
  "license": "MIT",
  "author": "k27dong",
  "type": "module",
  "main": "dist/Bamboo.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "pnpm build && pnpm start",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix && prettier --write .",
    "start": "node --enable-source-maps dist/Bamboo.js",
    "watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "deploy:dev": "tsx src/common/scripts/deploy.ts dev",
    "deploy:global": "tsx src/common/scripts/deploy.ts global"
  },
  "dependencies": {
    "@discordjs/voice": "^0.18.0",
    "bufferutil": "^4.0.8",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.13.0",
    "prettier": "^3.4.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  }
}
